{
    "name": "nix-develop",
    "version": "1.2.0",
    "main": "extension.js",
    "author": "Phyzait Lu",
    "engines": {
        "vscode": "^1.0.0"
    },
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.nixdevelopenable",
                "title": "Enable nix develop"
            },
            {
                "command": "extension.nixdevelopdisable",
                "title": "Disable nix develop"
            },
            {
                "command": "extension.nixdevelopupdate",
                "title": "Update environmental variables with nix develop"
            }
        ]
    },
    "license": "MIT",
    "repository": "https://github.com/hilaolu/nix-develop",
    "devDependencies": {
        "@vscode/vsce": "^2.22.0"
    }
}